{
  "ConnectionStrings": {
    "SqlConnection": "Server=(LocalDB)\\MSSQLLocalDB;Database=ProjectManagerDB;Integrated Security=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "ArslanProjectManager",
    "Audience": "ArslanProjectManagerUser",
    "SecurityKey": "type_your_secret_key_here_to_use_in_JWT", //type your key here
    "ExpirationInMinutes": 60
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowMinutes": 1
  }
}
